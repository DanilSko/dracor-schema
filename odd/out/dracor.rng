               <value>URL</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">URL</a:documentation>
               <value>wikidata</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">wikidata</a:documentation>
               <value>viaf</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">VIAF</a:documentation>
               <value>pnd</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">GND</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="tei_sourceDesc">
